{
	"name": "website",
	"version": "0.0.1",
	"private": true,
	"license": "Apache-2.0 with Commons Clause",
	"scripts": {
		"dev": "pnpm run codegen && vite dev --port 3000",
		"build": "pnpm run codegen && pnpm run svelte:check && vite build",
		"preview": "vite preview --port 3000",
		"lint": "pnpm run codegen && prettier --check \"**/*\" -u && eslint . && pnpm run svelte:check",
		"svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"svelte:check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write \"**/*\" -u",
		"codegen": "graphql-codegen --config codegen.ts"
	},
	"devDependencies": {
		"@eslint/js": "^9.10.0",
		"@fortawesome/free-brands-svg-icons": "^6.6.0",
		"@fortawesome/free-regular-svg-icons": "^6.6.0",
		"@graphql-codegen/cli": "^5.0.2",
		"@graphql-codegen/client-preset": "^4.3.3",
		"@graphql-codegen/typescript": "^4.0.9",
		"@graphql-codegen/typescript-document-nodes": "^4.0.9",
		"@graphql-typed-document-node/core": "^3.2.0",
		"@sveltejs/adapter-static": "^3.0.4",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"eslint": "^9.10.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.44.0",
		"globals": "^15.9.0",
		"graphql": "^16.9.0",
		"moment": "^2.30.1",
		"phosphor-svelte": "^2.0.1",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"sass": "^1.78.0",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.2",
		"svelte-fa": "^4.0.2",
		"svelte-i18n": "^4.0.0",
		"svelte2tsx": "^0.7.19",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.5.0",
		"vite": "^5.4.5"
	},
	"type": "module",
	"dependencies": {
		"@urql/exchange-auth": "^2.2.0",
		"@urql/svelte": "^4.2.1",
		"svelte-infinite-loading": "^1.4.0"
	}
}
